<Window x:Class="Linear_Programming_Calculator_Desktop.ResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Linear_Programming_Calculator_Desktop"
        mc:Ignorable="d"
        Title="Задачі лінійного програмування" Closed="Window_Closed" SizeToContent="Width" ResizeMode="CanMinimize" Height="450" Loaded="Window_Loaded" Width="800" Background="#F2F9F9F9">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Styles/LinearProgStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <StackPanel x:Name="SolutionPanel" Margin="10" />
        </ScrollViewer>
    </Grid>
</Window>
