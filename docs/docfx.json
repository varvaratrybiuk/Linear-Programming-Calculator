{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../../Linear Programming Calculator/Linear Programming Calculator Desktop",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "lp_calculator",
      "namespaceLayout": "nested",
      "filter": "filterConfig.yml"
    },
    {
      "src": [
        {
          "src": "../../Linear Programming Calculator/Methods",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "methods",
      "namespaceLayout": "nested",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Linear Programming Calculator Docs",
      "_appTitle": "Linear Programming Calculator Docs",
      "_enableSearch": false,
      "pdf": true
    }
  }
}